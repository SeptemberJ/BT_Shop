.deliveryAddress{
    height: 120rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 10rpx;
    background: #fff;
    border-top: 1px solid #FFF;
	border-image: url('../../image/strap.jpg') 30 30 round;
}
.chooseStore{
    width: calc(100% - 40rpx);
    height: 100rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    padding: 0 20rpx;
}
.chooseStoreText{
  width: 600rpx;
  display: inline-block;
  line-height: 100rpx;
  
}
.deliverTime{
    width: calc(100% - 40rpx);
    height: 100rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    padding: 0 20rpx;
}
.chooseStore image, .deliverTime image{
    width: 40rpx;
    height: 40rpx;
}
.noAddress{
    height: 120rpx;
    display: flex;
    justify-content:flex-start;
    align-items: center;
    padding: 10rpx;
    background: #fff;
    border-top: 1px solid #FFF;
	border-image: url('../../image/strap.jpg') 30 30 round;
}
.alignLeft{
    height: 100%;
    display: flex;
    justify-content:flex-start;
    align-items: center;
}
.add_address{
    width: 80rpx;
    height: 80rpx;
}
.alignLeft text{
    height: 100%;
    padding-left: 20rpx;
    line-height: 120rpx;
}
.addr_icon{
    width: 40rpx;
    height: 40rpx;
    display: flex;
}
.addr_icon image{
    width: 40rpx;
    height: 40rpx;
    display: block;
}
.addr_info{
    width: 670rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-content: center;
    justify-content: space-around;
    padding: 0  20rpx;
}
.addr_info .item{
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-between;

}
.detail{
    color: #666;
}
.addr_arrow{
    width: 40rpx;
    height: 40rpx;
    display: flex;
}
.addr_arrow image{
    width: 40rpx;
    height: 40rpx;
    display: block;
}
/*orderList*/
.orderList{
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-bottom: 20rpx solid #f5f5f5;
}
.order_item{
    width: calc(100% - 40rpx);
    height: 170rpx;
    margin-top: 20rpx;
    padding: 10rpx 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    background:#fff;

}
.ware_img{
    width: 150rpx;
    height: 150rpx;
    display: flex;
}
.ware_tit{
    width: calc(100% - 320rpx);
    height: 150rpx;
    display: flex;
    /*align-items: center;
    justify-content: center;*/
    padding-left: 20rpx;
    /*text-overflow: ellipsis;
    overflow: hidden;
    white-space: wrap;*/
}
.ware_amount_price{
    width: 150rpx;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items:flex-end;
    justify-content:space-around;
}
.ware_amount_price text{
    display: flex;
    color: #f60
}
.ware_amount_price text:nth-of-type(1)::before{
    content: "￥";
    
}
