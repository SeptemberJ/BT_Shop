/*hello customer*/
.hello{
    width: 100%;
    height: auto;
    display: flex;
    flex-direction: column;
    justify-content: center;
    background: #fff;
}
.nickName{
    height: auto;
    /*height: 120rpx;*/
    font-size: 50rpx;
    color: #b08957;
}
.Btabstract{
    font-size: 30rpx;
    color: #999999;
}
/*search*/
.searchBlock{
    width: 100%;
    height: 180rpx;
    display: flex;
    flex-direction: column;
    position: relative;
}
.search{
    width: calc(100% - 2px);
    height: 80rpx;
    margin: 50rpx 0;
    border: 1px solid #d8cdb0;
    border-radius: 15rpx;
    display: flex;
    align-items: center;
    background: #fafafa;
}
.search input{
    width: calc(100% - 60rpx - 20rpx - 40rpx);
    height: 60rpx;
    padding: 0 10rpx;
    color: #999999;
}
.search image{
    width: 60rpx;
    height: 60rpx;
}
/*resultList*/
.resultList{
    width: calc(100% - 2px);
    height: auto;
    background: #fff; 
    position: absolute;
    top: calc(50rpx + 80rpx + 10rpx);
    left: 0;
    display: flex;
    flex-direction: column;
    z-index: 9999;
    border: 1px solid #eee;
    
}
.resultList text{
    width: 100%;
    height: 50rpx;
    padding: 5rpx;
    line-height: 50rpx;
    text-overflow:ellipsis;
    overflow:hidden;
    white-space:nowrap;

}
